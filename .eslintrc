{
  "extends": "@antfu",
  "rules": {
    "no-console": "off",
    "eslint-comments/no-unlimited-disable": "off",
    "curly": [
      "error",
      "all"
    ],
    "vue/component-tags-order": [
      "error",
      {
        "order": [
          "route",
          "script",
          "template",
          "style"
        ]
      }
    ],
    "vue/first-attribute-linebreak": [
      "error",
      {
        "singleline": "ignore",
        "multiline": "ignore"
      }
    ]
  }
}
